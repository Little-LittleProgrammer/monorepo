{
  "name": "@q-front-npm/vue3-antd-pc-ui",
  "version": "1.1.0-beta.7",
  "description": "vue3,antd组件库",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://packages.aliyun.com/5f6426fcdb0493ecef9118ab/npm/npm-registry/"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./lib": {
      "types": "./dist/index.d.ts",
      "require": "./dist/lib/index.js"
    },
    "./es/*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/es/*.js"
    },
    "./lib/*": {
      "types": "./dist/src/*.d.ts",
      "require": "./dist/lib/*.js"
    },
    "./*": "./*"
  },
  "author": "Evan Wu",
  "license": "MIT",
  "dependencies": {
    "@q-front-npm/hooks": "workspace:*",
    "@q-front-npm/shared": "workspace:*",
    "@q-front-npm/utils": "workspace:*",
    "@q-front-npm/vue3-pc-ui": "workspace:*",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@q-front-npm-configs/vite": "workspace:*",
    "@q-front-npm/types": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "sass": "^1.63.3",
    "vue": "~3.3.4",
    "pinia": "^2.1.6",
    "@vue/test-utils": "^2.4.0",
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^4.0.1",
    "vue-router": "^4.2.4"
  },
  "peerDependencies": {
    "pinia": "^2.0.30",
    "vue": "~3.3.4",
    "vue-router": "^4.2.4"
  }
}
