{
    "name": "monorepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
      "apps/*",
      "packages/*",
      "docs/*",
      "configs/*"
    ],
    "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev --parallel",
      "lint": "turbo run lint"
    },
    "engines": {
      "npm": ">=7.0.0",
      "node": ">=14.0.0"
    },
    "packageManager": "pnpm@7.28.0",
    "dependencies": {
      "turbo": "1.7.2"
    },
    "devDependencies": {
      "@wuefront-configs/eslint": "workspace:*",
      "typescript": "4.9.5"
    },
    "pnpm": {
      "peerDependencyRules": {
        "ignoreMissing": [
          "@algolia/client-search",
          "@vue/composition-api"
        ]
      }
    }
  }